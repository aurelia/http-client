{
  "name": "aurelia-http-client",
  "version": "0.4.4",
  "description": "A simple, restful, message-based wrapper around XMLHttpRequest.",
  "keywords": [
    "aurelia",
    "http",
    "ajax"
  ],
  "homepage": "http://aurelia.io",
  "bugs": {
    "url": "https://github.com/aurelia/http-client/issues"
  },
  "license": "MIT",
  "author": "Rob Eisenberg <rob@bluespire.com> (http://robeisenberg.com/)",
  "main": "dist/commonjs/index.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/aurelia/http-client"
  },
  "jspm": {
    "main": "system/index",
    "format": "register",
    "directories": {
      "lib": "dist"
    },
    "dependencies": {
      "aurelia-path": "^0.4.2",
      "core-js": "npm:core-js@^0.4.1"
    }
  },
  "devDependencies": {
    "aurelia-tools": "^0.1.0",
    "conventional-changelog": "0.0.11",
    "del": "^1.1.0",
    "gulp": "^3.8.10",
    "gulp-6to5": "^3.0.0",
    "gulp-bump": "^0.1.11",
    "gulp-jshint": "^1.9.0",
    "gulp-yuidoc": "^0.1.2",
    "jasmine-core": "^2.1.3",
    "jshint-stylish": "^1.0.0",
    "karma": "^0.12.28",
    "karma-6to5-preprocessor": "^3.0.0",
    "karma-chrome-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.2",
    "karma-jasmine-ajax": "git://github.com/martingust/karma-jasmine-ajax.git",
    "karma-jspm": "^1.0.1",
    "object.assign": "^1.0.3",
    "require-dir": "^0.1.0",
    "run-sequence": "^1.0.2",
    "vinyl-paths": "^1.0.0"
  },
  "aurelia": {
    "usedBy": [],
    "documentation": {
      "links": [
        {
          "rel": "license",
          "mediaType": "text/plain",
          "title": "The MIT License (MIT)",
          "href": "LICENSE"
        },
        {
          "rel": "describedby",
          "mediaType": "application/aurelia-doc+json",
          "title": "API",
          "href": "doc/api.json"
        },
        {
          "rel": "version-history",
          "mediaType": "text/markdown",
          "title": "Change Log",
          "href": "doc/CHANGELOG.md"
        }
      ]
    }
  }
}
